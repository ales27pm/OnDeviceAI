# OnDeviceAI Environment Configuration
# Copy this file to .env and replace with actual values

# Project Configuration
EXPO_PUBLIC_VIBECODE_PROJECT_ID=your-project-id

# AI API Keys - Replace with actual keys
EXPO_PUBLIC_VIBECODE_OPENAI_API_KEY=sk-proj-YOUR-OPENAI-KEY
EXPO_PUBLIC_VIBECODE_ANTHROPIC_API_KEY=sk-ant-YOUR-ANTHROPIC-KEY
EXPO_PUBLIC_VIBECODE_GROK_API_KEY=xai-YOUR-GROK-KEY

# Legacy API Keys (for backward compatibility)
OPENAI_API_KEY=sk-proj-YOUR-OPENAI-KEY
ANTHROPIC_API_KEY=sk-ant-YOUR-ANTHROPIC-KEY
GROK_API_KEY=xai-YOUR-GROK-KEY

# Environment
NODE_ENV=development

# App Configuration
EXPO_PUBLIC_APP_NAME=OnDeviceAI
EXPO_PUBLIC_APP_VERSION=1.0.0

# GitHub Configuration (Optional)
GITHUB_USERNAME=your-github-username
GITHUB_REPOSITORY=your-repo-name
GITHUB_TOKEN=your-github-token
GITHUB_REPO_URL=https://github.com/your-username/your-repo.git
